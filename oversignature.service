[Unit]
Description=OverSignature ERP & Network Management System
After=network.target

[Service]
# Change this to your user
User=root
# Adjust the path to where the binary is located
ExecStart=/usr/local/bin/oversignature
Restart=always
Environment=NODE_ENV=production

[Install]
WantedBy=multi-user.target
